<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Act_FinPartida">

    <TextView
        android:id="@+id/twMensajeFinal"
        android:layout_width="320dp"
        android:layout_height="107dp"
        android:layout_marginTop="104dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/twNumFallos"
        android:layout_width="105dp"
        android:layout_height="50dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="91dp"
        android:text="TextView"
        app:layout_constraintEnd_toStartOf="@+id/btnVolver"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/twMensajeFinal" />

    <Button
        android:id="@+id/btnVolver"
        android:layout_width="132dp"
        android:layout_height="50dp"
        android:layout_marginStart="91dp"
        android:text="Button"
        app:layout_constraintStart_toEndOf="@+id/twNumFallos"
        app:layout_constraintTop_toTopOf="@+id/twNumFallos" />
</androidx.constraintlayout.widget.ConstraintLayout>